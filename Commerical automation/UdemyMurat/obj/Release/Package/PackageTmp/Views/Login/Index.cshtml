
@{
    Layout = null;
}
<!DOCTYPE html>
<html lang="en">
<head>
    <title>Login</title>
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta name="keywords" content="Project Responsive web template, Bootstrap Web Templates, Flat Web Templates, Android Compatible web template,
        Smartphone Compatible web template, free webdesigns for Nokia, Samsung, LG, SonyEricsson, Motorola web design" />
    <script type="application/x-javascript">
        addEventListener("load", function() { setTimeout(hideURLbar, 0); }, false);
               function hideURLbar(){ window.scrollTo(0,1); } </script>
    <link href="~/weblogin/fonts.googleapis.com/css?family=Open+Sans:300,300i,400,400i,600,600i,700,700i,800,800i&amp;subset=cyrillic,cyrillic-ext,greek,greek-ext,latin-ext,vietnamese" rel="stylesheet">
    <link href="~/weblogin/fonts.googleapis.com/css?family=Raleway:100,100i,200,200i,300,300i,400,400i,500,500i,600,600i,700,700i,800,800i,900,900i&amp;subset=latin-ext" rel="stylesheet">
    <link href="~/weblogin/css/style.css" type="text/css" rel="stylesheet" media="all">
    <link rel="stylesheet" href="~/weblogin/css/font-awesome.css">
    <link href="~/weblogin/css/popup-box.css" rel="stylesheet" type="text/css" media="all" />
</head>
<body>

    <section class="register">
        <div class="header">
            <div class="logo">
                <a href="#">GİRİŞ SƏHİFƏSİ </a>
            </div>
            <ul class="social">
                <li><a href="https://www.facebook.com/people/Zahir-Zairli/100006595188135/"><span class="fa fa-facebook"></span></a></li>
                <li><a href="https://www.facebook.com/people/Zahir-Zairli/100006595188135/"><span class="fa fa-twitter"></span></a></li>
                <li><a href="https://www.facebook.com/people/Zahir-Zairli/100006595188135/"><span class="fa fa-linkedin"></span></a></li>
                <li><a href="https://www.facebook.com/people/Zahir-Zairli/100006595188135/"><span class="fa fa-google-plus"></span></a></li>
            </ul>
            <div class="clear"></div>
        </div>

        <div class="register-full">
            <div class="register-left">
                <div class="register">
                    <p>Zairli Zahir</p>
                    <h1>Asp.Net Mvc5 İle</h1>
                    <h2>Web proje</h2>
                    <p>
                        Admin,işçi ve müştəri şəxsi kabinentləri mövcuddur.Sizə uyğun bölmədən şəxsi kabinentinizə daxil ola və ya qeydiyyatdan keçə bilərsiniz.
                    </p>
                    <div class="error" style="padding: 10px;margin-top: 10px;">
                        <small style="color:red;width: 10px;">@TempData["Message2"]</small>
                    </div>
                    <div class="content3">
                        <ul>
                            <li><a class="" href="#"> Markalar </a></li>
                            <li><a class="read" href="#"> Məhsullarimiz</a></li>
                        </ul>
                    </div>
                </div>
            </div>
            <div class="register-right">
                <div class="register-in">
                    <a class="book popup-with-zoom-anim button-isi zoomIn animated" data-wow-delay=".5s" href="#small-dialog2">qeydİyyatdan keç »</a>
                </div>
                <div class="register-in middle">
                    <a class="book popup-with-zoom-anim button-isi zoomIn animated" data-wow-delay=".5s" href="#small-dialog">müştərİ gİrİşİ »</a>
                </div>
                <div class="register-in">
                    <a class="book popup-with-zoom-anim button-isi zoomIn animated" data-wow-delay=".5s" href="#small-dialog1">İşcİ gİrİşİ »</a>
                </div>
                <div class="clear"> </div>
            </div>
            <div class="clear"> </div>
        </div>
    </section>
    <div class="pop-up">
        <div id="small-dialog" class="mfp-hide book-form">
            <div class="login-form login-form-left">
                <div class="agile-row">
                    <h3>Müştəri giriş səhifəsi</h3>
                    <div class="clear"></div>
                    <div class="login-agileits-top">
                        @using (Html.BeginForm("CariLogin", "Login", FormMethod.Post))
                        {
                            <input type="email" class="email" name="CariMail" Placeholder="Mail adresiniz" required="" />
                            <input type="password" class="password" name="CariSifre" Placeholder="Şifre" required="" />
                            <input type="submit" value="Daxil ol">
                        }
                    </div>
                    <div class="error" style="padding: 10px;margin-top: 10px;">
                        <small style="color:red;width: 10px;">@TempData["Message"]</small>
                    </div>
                    <br />
                    <div class="login-agileits-bottom">
                        <h6><a href="#">Forgot password?</a></h6>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <div class="pop-up">
        <div id="small-dialog1" class="mfp-hide book-form">

            <div class="login-form login-form-left">
                <div class="agile-row">
                    <h3>Isci giriş səhifəsi</h3>
                    <div class="clear"></div>
                    <div class="login-agileits-top">
                        @using (Html.BeginForm("AdminLogin", "Login", FormMethod.Post))
                        {
                            <input type="text" class="text" name="KullaniciAd" Placeholder="Adınız" required="" />
                            <input type="password" class="password" name="Sifre" Placeholder="Şifre" required="" />
                            <input type="submit" value="Daxil ol">
                        }
                    </div>
                    <div class="error" style="padding: 10px;margin-top: 10px;">
                        <small style="color:red;width: 10px;">@TempData["Message3"]</small>
                    </div>
                    <div class="login-agileits-bottom">
                        <h6><a href="#">Forgot password?</a></h6>
                    </div>
                </div>
            </div>

        </div>
    </div>
    <div class="pop-up">
        <div id="small-dialog2" class="mfp-hide book-form">
            @Html.Action("Partial1", "Login")
        </div>
    </div>
    <script type="text/javascript" src="~/weblogin/js/jquery-2.1.4.min.js"></script>
    <script src="~/weblogin/js/jquery.magnific-popup.js" type="text/javascript"></script>
    <script>
        $(document).ready(function () {
            $('.popup-with-zoom-anim').magnificPopup({
                type: 'inline',
                fixedContentPos: false,
                fixedBgPos: true,
                overflowY: 'auto',
                closeBtnInside: true,
                preloader: false,
                midClick: true,
                removalDelay: 300,
                mainClass: 'my-mfp-zoom-in'
            });

        });
    </script>
</body>
</html>
